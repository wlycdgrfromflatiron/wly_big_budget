<%= form_with scope: @tag, url: form_url, method: submit_method do |f| %>
  <%= wly_form_div self, 'wly_labeled_text_field', f, :name %>
  <%= wly_check_boxes_for @prefab_stores, :prefab_store_ids, "Stores", f %>
  <%= wly_check_boxes_for @prefab_items, :prefab_item_ids, "Items", f %>
  <%= f.submit %>
<% end %>
